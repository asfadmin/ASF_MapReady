			      <xs:documentation>ECR coordinate velocity in y [m/s]</xs:documentation>
			    </xs:annotation>
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:double">
                                  <xs:attribute name="units" type="xs:string" fixed="m/s"/>
