                                <xs:element name="polynomialDegree" type="xs:integer">
                                  <xs:annotation>
                                    <xs:documentation>polynomial degree</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="basebandDoppler">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="coefficient" minOccurs="3" maxOccurs="3">
