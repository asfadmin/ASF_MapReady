	<xs:element name="processing">
          <xs:annotation>
            <xs:documentation>processor and product configuration parameters</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
	      <xs:element name="doppler">
                <xs:complexType>
                  <xs:sequence>
