<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">

  <!-- definitions of types used in the schema //-->
  <xs:simpleType name="string20">
    <xs:restriction base="xs:string">
      <xs:maxLength value="20"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string80">
    <xs:restriction base="xs:string">
      <xs:maxLength value="80"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string128">
    <xs:restriction base="xs:string">
      <xs:maxLength value="128"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string255">
    <xs:restriction base="xs:string">
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string1024">
    <xs:restriction base="xs:string">
      <xs:maxLength value="1024"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="extUnits">
    <xs:restriction base="xs:string">
      <xs:enumeration value="m"/>
      <xs:enumeration value="s"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="latitudeDegree">
    <xs:restriction base="xs:float">
      <xs:minInclusive value="-180"/>
      <xs:maxInclusive value="180"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="lat">
    <xs:annotation>
      <xs:documentation>geographical latitude [degrees]</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="latitudeDegree">
          <xs:attribute name="units" type="xs:string" fixed="degrees"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="longitudeDegree">
    <xs:restriction base="xs:float">
      <xs:minInclusive value="-180"/>
      <xs:maxInclusive value="180"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="dataFormat">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CEOS"/>
      <xs:enumeration value="GEOTIFF"/>
      <xs:enumeration value="HDF5"/>
      <xs:enumeration value="COSAR"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="lon">
    <xs:annotation>
      <xs:documentation>geographical longitude [degrees]</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="longitudeDegree">
          <xs:attribute name="units" type="xs:string" fixed="degrees"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="polLayer">
    <xs:annotation>
      <xs:documentation>HH, HV, VH or VV</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="HH"/>
        <xs:enumeration value="HV"/>
        <xs:enumeration value="VH"/>
        <xs:enumeration value="VV"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
     </xs:simpleType>
  </xs:element>
  <xs:element name="imagingMode">
    <xs:annotation>
      <xs:documentation>FINE BEAM, STANDARD BEAM, STRIPMAP, SCANSAR or SPOTLIGHT</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="FINE BEAM"/>
        <xs:enumeration value="STANDARD BEAM"/>
        <xs:enumeration value="STRIPMAP"/>
        <xs:enumeration value="SCANSAR"/>
        <xs:enumeration value="SPOTLIGHT"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="lookDirection">
    <xs:annotation>
      <xs:documentation>LEFT or RIGHT</xs:documentation>
    </xs:annotation>  
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="LEFT"/>
        <xs:enumeration value="RIGHT"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="polarizationMode">
    <xs:annotation>
      <xs:documentation>SINGLE, DUAL or QUAD</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="SINGLE"/>
        <xs:enumeration value="DUAL"/>
        <xs:enumeration value="QUAD"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:simpleType name="product">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SLC"/>
      <xs:enumeration value="STD"/>
      <xs:enumeration value="TC"/>
      <xs:enumeration value="RTC"/>
      <xs:enumeration value="GEO"/>
      <xs:enumeration value="SSC"/>
      <xs:enumeration value="MGD"/>
      <xs:enumeration value="GEC"/>
      <xs:enumeration value="EEC"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="projection">
    <xs:annotation>
      <xs:documentation>GROUNDRANGE, SLANTRANGE or MAP</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="SLANTRANGE"/>
        <xs:enumeration value="GROUNDRANGE"/>
        <xs:enumeration value="MAP"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:simpleType name="mapProjection">
    <xs:restriction base="xs:string">
      <xs:enumeration value="UTM"/>
      <xs:enumeration value="POLARSTEREOGRAPHIC"/>
      <xs:enumeration value="GEOGRAPHIC"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="resolutionVariant">
    <xs:annotation>
      <xs:documentation>SE or RE</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="SE"/>
        <xs:enumeration value="RE"/>
        <xs:enumeration value="UNDEFINED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:simpleType name="coordinateType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="RAW"/>
      <xs:enumeration value="ZERODOPPLER"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="dataStart">
    <xs:restriction base="xs:string">
      <xs:enumeration value="EARLYAZNEARRG"/>
      <xs:enumeration value="EARLYAZFARRG"/>
      <xs:enumeration value="LATEAZNEARRG"/>
      <xs:enumeration value="LATEAZFARRG"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="resamplingMethod">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NEAREST NEIGHBOR"/>
      <xs:enumeration value="BILINEAR"/>
      <xs:enumeration value="CUBIC CONVOLUTION"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="orbitAccuracy">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PREDICTED"/>
      <xs:enumeration value="RESTITUTED"/>
      <xs:enumeration value="PRECISE"/>
      <xs:enumeration value="TLE"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="processingLevel">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PRE-PROCESSING"/>
      <xs:enumeration value="LEVEL_ZERO"/>
      <xs:enumeration value="LEVEL_ONE"/>
      <xs:enumeration value="LEVEL_TWO"/>
      <xs:enumeration value="UNDEFINED"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="referenceType">
    <xs:choice>
      <xs:element name="frameID" type="xs:integer">
        <xs:annotation>
          <xs:documentation>frame number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sceneExtent"/>
      <xs:element name="sceneCenterCoord"/>
    </xs:choice>
  </xs:complexType>

  <!-- Start of the actual schema //-->
  <xs:element name="level1Product">
    <xs:annotation>
      <xs:documentation>contains the metadata for level 1 SAR product</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="generalHeader">
          <xs:annotation>
            <xs:documentation>ground segment general header structure</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="itemName" type="string80">
                <xs:annotation>
                  <xs:documentation>contains the name of the exchanged product</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="mission" type="string20">
                <xs:annotation>
                  <xs:documentation>indicates the mission, for which the product is provided </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="source" type="string20">
                <xs:annotation>
                  <xs:documentation>specifies the sensor, source or originator for the provided product</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="destination" type="string20">
                <xs:annotation>
                  <xs:documentation>specifies the destination to which provided the product goes</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="generationSystem" type="string80">
                <xs:annotation>
                  <xs:documentation>specifies the system which generated the product</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="generationTime" type="xs:dateTime">
                <xs:annotation>
                  <xs:documentation>reports UTC time at which the product was generated</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="referenceDocument" type="string255" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>specifies the reference document, in which the product is specified with respect to contents and format</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="revision" type="string20" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>handle to distinguish between project phases (e.g. testing, commissioning phase, operating)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="revisionComment" type="string1024" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>remark field for a narrative description of the revision field</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="fileName" type="xs:string" use="required"/>
            <xs:attribute name="fileVersion" type="xs:float" use="optional"/>
            <xs:attribute name="status" type="xs:string" use="optional"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="productComponents">
          <xs:annotation>
            <xs:documentation>lists and points to the product components</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="annotation" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>pointer to the annoation file(s)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="type">
                      <xs:annotation>
                        <xs:documentation>annotation file type</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="MAIN"/>
                          <xs:enumeration value="GEOREF"/>
                          <xs:enumeration value="GEOCODE"/>
                          <xs:enumeration value="OTHER"/>
                          <xs:enumeration value="UNDEFINED"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="file">
                      <xs:annotation>
                        <xs:documentation>pointer to product components</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="location">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="host" type="string1024">
                                <xs:annotation>
                                  <xs:documentation>host where the file is located; default to: .</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                                <xs:element name="path" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>path where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="filename" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>name of the file</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="size" type="xs:long">
                            <xs:annotation>
                              <xs:documentation>size of the file</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="imageData" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>containing one or more polarimetric channels in separate binary data matrices</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="polLayer"/>
                    <xs:element name="file">
                      <xs:annotation>
                        <xs:documentation>pointer to product components</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="location">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="host" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>host where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="path" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>path where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="filename" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>name of the file</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="size" type="xs:long">
                            <xs:annotation>
                              <xs:documentation>size of the file</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="layerIndex" type="xs:integer"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="quicklooks" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>rescaled images for each image layer</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="polLayer"/>
                    <xs:element name="file">
                      <xs:annotation>
                        <xs:documentation>pointer to product components</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="location">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="host" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>host where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="path" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>path where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="filename" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>name of the file</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="size" type="xs:long">
                            <xs:annotation>
                              <xs:documentation>size of the file</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="layerIndex" type="xs:integer"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="browseImage">
                <xs:annotation>
                  <xs:documentation>one low resolution quicklook</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="file">
                      <xs:annotation>
                        <xs:documentation>pointer to product components</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="location">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="host" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>host where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="path" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>path where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="filename" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>name of the file</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="size" type="xs:long">
                            <xs:annotation>
                              <xs:documentation>size of the file</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="mapPlot">
                <xs:annotation>
                  <xs:documentation>geographic map showing the image footprint</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="file">
                      <xs:annotation>
                        <xs:documentation>pointer to product components</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="location">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="host" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>host where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="path" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>path where the file is located; default to: .</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="filename" type="string1024">
                                  <xs:annotation>
                                    <xs:documentation>name of the file</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="size" type="xs:long">
                            <xs:annotation>
                              <xs:documentation>size of the file</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="productInfo">
          <xs:annotation>
            <xs:documentation>general mission, scene, imaging mode related parameters</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="generationInfo">
                <xs:annotation>
                  <xs:documentation>key parameters of the product generation and delivery</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="logicalProductID" type="string1024">
                      <xs:annotation>
                        <xs:documentation>logical ID</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="receivingStation" type="string20">
                      <xs:annotation>
                        <xs:documentation>receiving ground station</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="level0ProcessingFacility" type="string20">
                      <xs:annotation>
                        <xs:documentation>facility processing the level zero data</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="level1ProcessingFacility" type="string20">
                      <xs:annotation>
                        <xs:documentation>facility processing the level one data</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="groundOperationsType">
                      <xs:annotation>
                        <xs:documentation>OPERATIONAL, PRE-OPERATIONAL, INSTRUMENT or TEST</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="OPERATIONAL"/>
                          <xs:enumeration value="PRE-OPERATIONAL"/>
                          <xs:enumeration value="INSTRUMENT"/>
                          <xs:enumeration value="TEST"/>
                          <xs:enumeration value="UNDEFINED"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="deliveryInfo" type="string1024">
                      <xs:annotation>
                        <xs:documentation>delivery information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="copyrightInfo" type="string1024">
                      <xs:annotation>
                        <xs:documentation>general copyright remark</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="qualityInfo">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="qualityInspection">
                            <xs:annotation>
                              <xs:documentation>AUTO APPROVED, OPERATOR APPROVED or NOT APPROVED</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="AUTO APPROVED"/>
                                <xs:enumeration value="OPERATOR APPROVED"/>
                                <xs:enumeration value="NOT APPROVED"/>
                                <xs:enumeration value="UNDEFINED"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="missionInfo">
                <xs:annotation>
                  <xs:documentation>mission and orbit parameters</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="mission" type="string1024">
                      <xs:annotation>
                        <xs:documentation>mission abbreviation</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="orbitPhase" type="xs:integer">
                      <xs:annotation>
                        <xs:documentation>orbit phase: 1 prelaunch phase, 0 launch phase, 1 nominal orbit</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="orbitCycle" type="xs:integer">
                      <xs:annotation>
                        <xs:documentation>cycle number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="absOrbit" type="xs:integer">
                      <xs:annotation>
                        <xs:documentation>absolute orbit number at the start of the scene</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="relOrbit" type="xs:integer">
                      <xs:annotation>
                        <xs:documentation>relative orbit</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="numOrbitsInCycle" type="xs:integer">
                      <xs:annotation>
                        <xs:documentation>nominal number of orbits per cycle</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="orbitDirection">
                      <xs:annotation>
                        <xs:documentation>ASCENDING or DESCENDING</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="ASCENDING"/>
                          <xs:enumeration value="DESCENDING"/>
                          <xs:enumeration value="UNDEFINED"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="acquisitionInfo">
                <xs:annotation>
                  <xs:documentation>SAR sensor configuration and instrument modes during acquisition</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="sensor" type="string20">
                      <xs:annotation>
                        <xs:documentation>sensor identifier: SAR</xs:documentation>
                      </xs:annotation>
                    </xs:element>
