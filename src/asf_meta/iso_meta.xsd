	<xs:element name="productQuality">
          <xs:annotation>
            <xs:documentation>summarizes image and data quality</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
	      <xs:element name="rawDataQuality">
	        <xs:annotation>
		  <xs:documentation>assessment of raw data quality (per layer)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
		  <xs:sequence>
		    <xs:element ref="polLayer"/>
		    <xs:element name="beamID" type="string20">
		      <xs:annotation>
			<xs:documentation>beam identifier</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		    <xs:element name="numGaps" type="xs:integer">
		      <xs:annotation>
			<xs:documentation>number of data gaps</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		    <xs:element name="gap">
		      <xs:annotation>
		        <xs:documentation>data gap information</xs:documentation>
		      </xs:annotation>
		      <xs:complexType>
			<xs:sequence>
			  <xs:element name="start" type="xs:unsignedLong">
			    <xs:annotation>
			      <xs:documentation>start line of data gap</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="length" type="xs:integer">
			    <xs:annotation>
			      <xs:documentation>number of lines forming data gap</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="fill">
			    <xs:annotation>
			      <xs:documentation>RANDOM or ZERO</xs:documentation>
			    </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="RANDOM"/>
                                <xs:enumeration value="ZERO"/>
                                <xs:enumeration value="UNDEFINED"/>
                              </xs:restriction>
                            </xs:simpleType>
			  </xs:element>
			</xs:sequence>
                        <xs:attribute name="num" type="xs:integer"/>
		      </xs:complexType>
		    </xs:element>
		    <xs:element name="gapSignificanceFlag" type="xs:boolean">
		      <xs:annotation>
		        <xs:documentation>gaps above the tolerance level?</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		    <xs:element name="missingLinesSignificanceFlag" type="xs:boolean">
		      <xs:annotation>
		        <xs:documentation>missing lines outside tolerance level?</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		    <xs:element name="bitErrorSignificanceFlag" type="xs:boolean">
		      <xs:annotation>
		        <xs:documentation>bit error outside tolerance limits?</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		    <xs:element name="timeReconstructionSignificanceFlag" type="xs:boolean">
		      <xs:annotation>
		        <xs:documentation>time reconstruction outside tolerance limits?</xs:documentation>
		      </xs:annotation>
		    </xs:element>
		  </xs:sequence>
		</xs:complexType>
	      </xs:element>
	      <xs:element name="processingParameterQuality">
                <xs:complexType>
                  <xs:annotation>
                    <xs:documentation>quality of processing parameters</xs:documentation>
                  </xs:annotation>
                  <xs:sequence>
                    <xs:element name="dopplerAmbiguityNotZeroFlag" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation>Doppler ambiguities?</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="dopplerOutsideLimitsFlag" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation>Doppler outside tolerance limits?</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="geolocationQualityLowFlag" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation>orbit/attitude/DEM/Doppler quality problems?</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
	     </xs:element>
	     <xs:element name="imageDataQuality">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="polLayer"/>
                    <xs:element name="beamID" type="string20">
                      <xs:annotation>
                        <xs:documentation>beam identifier</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="imageDataStatistics">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:annotation>
                            <xs:documentation>image data statistics</xs:documentation>
                          </xs:annotation>
                          <xs:element name="minValue" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>minimum value</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="maxValue" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>maximum value</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="meanValue" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>mean value</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="standardDeviation" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>standard deviation</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="missingLines" type="xs:int">
                            <xs:annotation>
                              <xs:documentation>missing lines</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="bitErrorRate" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>bit error rate</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="noDataValue" type="xs:double">
                            <xs:annotation>
                              <xs:documentation>no data value</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="layerIndex" type="xs:integer"/>
                </xs:complexType>
	      </xs:element>
	      <xs:element name="limits">
		<xs:complexType>
		  <xs:sequence>
		    <xs:element name="rawData">
		      <xs:complexType>
			<xs:sequence>
			  <xs:element name="gapDefinition" type="xs:integer">
			    <xs:annotation>
			      <xs:documentation>number of consecutive missing lines considered a gap</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="gapPercentageLimit" type="xs:double">
			    <xs:annotation>
			      <xs:documentation>percentage of gaps considered significant</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="missingLinePercentageLimit" type="xs:double">
			    <xs:annotation>
			      <xs:documentation>percentage of missing lines considered significant</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="bitErrorLimit" type="xs:double">
			    <xs:annotation>
			      <xs:documentation>bit error rate considered significant</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			  <xs:element name="timeReconstructionPercentageLimit" type="xs:double">
			    <xs:annotation>
			      <xs:documentation>percentage of time reconstruction considered significant</xs:documentation>
			    </xs:annotation>
			  </xs:element>
			</xs:sequence>
		      </xs:complexType>
		    </xs:element>
		    <xs:element name="processing">
		      <xs:complexType>
			<xs:sequence>
		 	  <xs:element name="dopplerCentroidLimit">
			    <xs:annotation>
			      <xs:documentation>Doppler value limit [Hz]</xs:documentation>
			    </xs:annotation>
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:double">
                                  <xs:attribute name="units" type="xs:string" fixed="Hz"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
			  </xs:element>
			  <xs:element name="geolocationQualityLimit">
			    <xs:annotation>
			      <xs:documentation>minimum posting required for accurate geolocation [arcsec]</xs:documentation>
			    </xs:annotation>
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:double">
                                  <xs:attribute name="units" type="xs:string" fixed="arcsec"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
			  </xs:element>
			</xs:sequence>
		      </xs:complexType>
		    </xs:element>
		  </xs:sequence>
		</xs:complexType>
	      </xs:element>
	      <xs:element name="instrumentStateRemark" type="string1024">
		<xs:annotation>
		  <xs:documentation>steering or antenna problems</xs:documentation>
		</xs:annotation>
	      </xs:element>
	    </xs:sequence>
	  </xs:complexType>
	</xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
